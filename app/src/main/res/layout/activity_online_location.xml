<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.wknn_location.OnlineLocationActivity">

    <TextView
        android:id="@+id/Online_2"
        android:layout_width="92dp"
        android:layout_height="27dp"
        android:hint="Online_2"
        app:layout_constraintLeft_toRightOf="@+id/Online_1"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/Online_1"
        android:layout_width="95dp"
        android:layout_height="27dp"
        android:hint="online_1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/Kalman"
        android:layout_width="63dp"
        android:layout_height="35dp"
        android:text="Kalm"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/clear"
        app:layout_constraintLeft_toRightOf="@+id/InputDB"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/clear"
        android:layout_width="61dp"
        android:layout_height="37dp"
        android:text="Clear"
        app:layout_constraintLeft_toRightOf="@+id/Online_2"
        android:layout_marginLeft="9dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <Button
        android:id="@+id/InputDB"
        android:layout_width="66dp"
        android:layout_height="36dp"
        android:text="InputDB"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/clear"
        app:layout_constraintLeft_toRightOf="@+id/locationTr"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/locationTr"
        android:layout_width="64dp"
        android:layout_height="35dp"
        android:text="LOCT"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Online_2"
        app:layout_constraintLeft_toRightOf="@+id/location"
        android:layout_marginLeft="8dp" />

    <Button
        android:id="@+id/Scan_online"
        android:layout_width="73dp"
        android:layout_height="36dp"
        android:text="SO"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/clear"
        android:layout_marginLeft="18dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/location"
        android:layout_width="71dp"
        android:layout_height="35dp"
        android:text="LocF"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Online_1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageView
        android:id="@+id/picture_online"
        android:layout_width="345dp"
        android:layout_height="395dp"

        android:layout_marginBottom="8dp"
        android:layout_marginLeft="16dp"
        android:src="@drawable/rtoom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />
</android.support.constraint.ConstraintLayout>
